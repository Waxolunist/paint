{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "max-len": [2, {"code": 110, "tabWidth": 4, "ignoreUrls": true}],
        "quotes": ["error", "single", { "allowTemplateLiterals": true }]
    }
}
